    "moment": "^2.30.1",
    "moment-timezone": "^0.5.47",
    "nanoid": "^5.0.9",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-confetti": "^6.1.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-day-picker": "^8.10.1",
