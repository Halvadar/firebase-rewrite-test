{
  "name": "client",
  "private": true,
  "public_url": "app.html",
  "type": "module",
  "scripts": {
    "dev": "yarn start",
    "start": "vite",
    "preview": "vite preview --port 3000",
    "build": "tsc && vite build && mv build/index.html build/app.html",
    "build:staging": "tsc && vite build --mode staging && mv build/index.html build/app.html",
    "analyze": "tsc && vite build && open stats.html",
    "deploy": "sh scripts/deploy.sh",
    "lint": "prettier src --write && eslint src --ext .js,.jsx",
    "type-check": "tsc --noEmit",
    "upgrade": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
