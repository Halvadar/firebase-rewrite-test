    "@stripe/react-stripe-js": "^3.0.0",
    "@stripe/stripe-js": "^5.2.0",
    "@tanstack/react-query": "^5.62.3",
    "@tippyjs/react": "^4.2.6",
    "@typeform/embed-react": "^4.2.0",
    "@uidotdev/usehooks": "^2.4.1",
    "@vaadin/vaadin-time-picker": "3.1.0",
    "@vvo/tzdb": "^6.161.0",
